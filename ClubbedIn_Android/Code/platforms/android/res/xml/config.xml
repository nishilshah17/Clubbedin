<?xml version='1.0' encoding='utf-8'?>
<widget id="com.RNI.clubbedin" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>
        Clubbed In
    </name>
    <description>
        Helping YOU get organized.
    </description>
    <author email="clubbedinapp@gmail.com" href="https://clubbedinapp.com">
        RNI
    </author>
    <gap:platforms>
        <gap:platform minVersion="4.0" name="ios" />
        <gap:platform minVersion="2.1" name="android" />
    </gap:platforms>
    <preference name="phonegap-version" value="2.9.0" />
    <preference name="orientation" value="portrait" />
    <icon src="icon.png" />
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="http://api.phonegap.com/1.0/camera" />
    <feature name="http://api.phonegap.com/1.0/file" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <feature name="http://api.phonegap.com/1.0/notification" />
    <access origin="*" />
</widget>
